! special
*.foreground:   #c5c8c6
*.background:   #000000
*.cursorColor:  #c5c8c6

! black
*.color0:   #000000
*.color8:   #767676

!red
*.color1:   #cc0403
*.color9:   #f2201f

!green
*.color2:   #19cb00
*.color10:  #23fd00

!yellow
*.color3:   #cecb00
*.color11:  #fffd00

!blue
*.color4:   #0d73cc
*.color12:  #1a8fff

!magenta
